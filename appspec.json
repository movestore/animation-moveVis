    {
      "id": "map_token",
      "name": "Map service API key",
      "description": "API Key for your account with the given map service provider. The following services require API authorization: Stamen, Stadia, Thunderforest, Mapbox, MapTiler. If requesting a basemap from one of these providers, you must provide your API token here. See the map provider's website for more information about creating an API key.",
      "defaultValue": null,
      "type": "SECRET"
    },
    {
      "id": "map_res",
      "name": "Resolution of background map",
      "description": "Select the resolution for your background map. This value must be between 0 and 1. Take into account the extent of your data set (i.e. high resolution is not necessary for far ranging data). Note that selection of a too high or too low value might lead to different error. Please try out different values if in doubt.",
      "defaultValue": 1,
      "type": "DOUBLE"
    },
    {
      "id": "margin_factor",
      "name": "Adaption factor for map extent",
      "description": "Insert the adaption factor by which to multiply the map extent. Anything between 0.1 and 1 decreases, above 1 increases the extent. By default, the extent of the data is used, i.e. 1.",
      "defaultValue": 1,
      "type": "DOUBLE"
    },
    {
      "id": "equidistant",
      "name": "Make plot margins equidistant",
      "description": "Enable this option to make the output map extent (approximately) equidistant in the x- and y-dimensions. This can be useful when visualizing data that are highly skewed along one axis. Note that enabling this option may lead to strong distortions for maps that cover polar regions.",
      "defaultValue": false,
      "type": "CHECKBOX"
    },
    {
      "id": "col_opt",
      "name": "Track colour option",
      "description": "Select how you want to colour your animated tracks.",
      "defaultValue": "one",
      "type": "RADIOBUTTONS",
      "options": [
        {
          "value": "one",
          "displayText": "Single colour (red) for all tracks"
